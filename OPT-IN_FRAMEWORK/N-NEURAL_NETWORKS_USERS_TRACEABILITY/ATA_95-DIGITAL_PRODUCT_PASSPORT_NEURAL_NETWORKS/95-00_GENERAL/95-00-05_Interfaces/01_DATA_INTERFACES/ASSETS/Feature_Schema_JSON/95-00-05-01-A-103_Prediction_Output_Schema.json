{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Model Prediction Output Schema",
  "type": "object",
  "properties": {
    "prediction_id": {
      "type": "string",
      "format": "uuid"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "model_id": {
      "type": "string"
    },
    "model_version": {
      "type": "string"
    },
    "prediction": {
      "type": "object"
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "explanation": {
      "type": "object"
    }
  },
  "required": ["prediction_id", "timestamp", "model_id", "prediction", "confidence"]
}
