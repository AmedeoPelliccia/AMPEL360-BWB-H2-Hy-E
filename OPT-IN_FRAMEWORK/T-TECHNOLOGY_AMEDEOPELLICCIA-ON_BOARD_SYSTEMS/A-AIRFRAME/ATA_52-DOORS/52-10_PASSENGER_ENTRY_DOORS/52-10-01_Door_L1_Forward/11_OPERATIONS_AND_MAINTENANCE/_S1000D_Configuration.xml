    <description>
      <levelledPara>
        <title>Configuration Overview</title>
        <para>This S1000D configuration defines the data module structure and business rules for the Door L1 Forward operations and maintenance documentation.</para>
      </levelledPara>
    </description>

    <configuration>
      <projectInfo>
        <projectName>AMPEL360 BWB H2 Hy-E Aircraft</projectName>
        <projectCode>AMPEL360</projectCode>
        <ataChapter>52-10-01</ataChapter>
        <systemName>Door L1 Forward - Passenger Entry Door</systemName>
      </projectInfo>

      <s1000dIssue>
        <version>5.0</version>
        <amendment>0</amendment>
      </s1000dIssue>

      <modelIdentification>
        <modelIdentCode>AMPEL360</modelIdentCode>
        <systemDiffCode>A</systemDiffCode>
      </modelIdentification>

      <dataModuleTypes>
        <descriptive infoCodeRange="000-099">
          <description>Descriptive information including system overview, technical data, and CAOS integration</description>
        </descriptive>
        <procedural infoCodeRange="200-299">
          <description>Operational procedures including normal, emergency, and manual operations</description>
        </procedural>
        <procedural infoCodeRange="500-599">
          <description>Maintenance procedures including removal, installation, and adjustment</description>
        </procedural>
        <procedural infoCodeRange="700-799">
          <description>Testing and inspection procedures</description>
        </procedural>
        <process infoCodeRange="900-999">
          <description>Troubleshooting, diagnostics, and CAOS-enhanced processes</description>
        </process>
      </dataModuleTypes>

      <caosIntegration>
        <enabled>true</enabled>
        <features>
          <feature name="RealTimeMonitoring">Active</feature>
          <feature name="PredictiveMaintenance">Active</feature>
          <feature name="DigitalTwin">Active</feature>
          <feature name="FleetLearning">Active</feature>
          <feature name="DynamicPublications">Active</feature>
        </features>
        <dataFlowRate>
          <sensorData unit="Hz">100</sensorData>
          <diagnosticData unit="Hz">10</diagnosticData>
          <analyticsData unit="Hz">1</analyticsData>
        </dataFlowRate>
      </caosIntegration>

      <publicationModules>
        <publicationModule id="PMC-AMPEL360-52-00-00-00A-00WA-D">
          <title>Door Systems Maintenance Manual</title>
          <type>Maintenance Manual</type>
          <format>IETP</format>
