{
  "digital_twin_config": {
    "version": "1.0",
    "date_created": "2025-11-05",
    "aircraft_model": "AMPEL360 BWB H2 Hy-E Q100",
    "ata_chapter": "02-10-00",
    "description": "Digital twin configuration for aircraft general data integration with CAOS system"
  },
  
  "aircraft_parameters": {
    "reference_file": "Aircraft_Model_Parameters.yaml",
    "parameter_categories": [
      "aircraft_dimensions",
      "aircraft_weights",
      "center_of_gravity",
      "propulsion_system",
      "performance_limits",
      "environmental_performance"
    ]
  },
  
  "real_time_data_streams": {
    "position_tracking": {
      "enabled": true,
      "update_frequency_hz": 1,
      "accuracy_requirement_m": 5,
      "data_sources": [
        "GPS/GNSS primary",
        "GPS/GNSS backup",
        "Inertial Reference System"
      ],
      "fusion_algorithm": "Kalman filter with sensor weighting"
    },
    
    "weight_and_balance": {
      "enabled": true,
      "update_frequency_hz": 0.1,
      "cg_accuracy_percent_mac": 0.5,
      "fuel_accuracy_kg": 20,
      "data_sources": [
        "H2 fuel quantity sensors (6 tanks)",
        "Fuel transfer system status",
        "Loading system interface",
        "Predictive consumption model"
      ],
      "cg_limits": {
        "forward_limit_percent_mac": 15.0,
        "aft_limit_percent_mac": 42.0,
        "alert_threshold_percent_mac": 2.0
      }
    },
    
    "fuel_cell_monitoring": {
      "enabled": true,
      "update_frequency_hz": 10,
      "stacks_monitored": 6,
      "parameters_per_stack": [
        "power_output_mw",
        "efficiency_percent",
        "temperature_celsius",
        "h2_flow_rate_kg_s",
        "voltage",
        "current",
        "health_status"
      ],
      "alert_conditions": {
        "power_deviation_percent": 10,
        "temperature_high_celsius": 85,
        "temperature_low_celsius": 55,
        "efficiency_low_percent": 50
      }
    },
    
    "propulsion_system": {
      "enabled": true,
      "update_frequency_hz": 10,
      "motors_monitored": 12,
      "parameters_per_motor": [
        "rpm",
        "torque_knm",
        "power_output_mw",
        "temperature_celsius",
        "vibration_level",
        "efficiency_percent"
      ],
      "total_thrust_monitoring": true,
      "thrust_symmetry_monitoring": true
    },
    
    "hydrogen_fuel_system": {
      "enabled": true,
      "update_frequency_hz": 1,
      "tanks_monitored": 6,
      "parameters_per_tank": [
        "quantity_kg",
        "temperature_celsius",
        "pressure_bar",
        "boiloff_rate_percent_day",
        "fill_level_percent"
      ],
      "total_fuel_remaining_kg": true,
      "reserve_monitoring": {
        "minimum_reserve_kg": 500,
        "alert_threshold_kg": 600
      }
    },
    
    "performance_monitoring": {
      "enabled": true,
      "update_frequency_hz": 0.1,
      "parameters": [
        "airspeed_kias",
        "mach_number",
        "altitude_ft",
        "vertical_speed_fpm",
        "heading_deg",
        "attitude_pitch_deg",
        "attitude_roll_deg",
        "flight_path_angle_deg"
      ],
      "performance_calculations": {
        "fuel_burn_rate": true,
        "range_remaining": true,
        "endurance_remaining": true,
        "optimal_altitude": true,
        "optimal_speed": true
      }
    },
    
    "environmental_sensors": {
      "enabled": true,
      "update_frequency_hz": 1,
      "parameters": [
        "outside_air_temperature_celsius",
        "static_air_pressure_hpa",
        "wind_speed_kt",
        "wind_direction_deg",
        "turbulence_level",
        "icing_conditions"
      ]
    },
    
    "structural_health": {
      "enabled": true,
      "update_frequency_hz": 100,
      "monitoring_points": 250,
      "parameters": [
        "strain_gauges",
        "vibration_sensors",
        "temperature_sensors",
        "acoustic_sensors"
      ],
      "fatigue_tracking": true,
      "anomaly_detection": true
    }
  },
  
  "digital_twin_models": {
    "aerodynamic_model": {
      "enabled": true,
      "model_type": "CFD-validated lookup tables with interpolation",
      "update_frequency_hz": 0.1,
      "inputs": [
        "airspeed",
        "altitude",
        "weight",
        "cg_position",
        "configuration"
      ],
      "outputs": [
        "lift_coefficient",
        "drag_coefficient",
        "pitch_moment",
        "required_thrust"
      ],
      "accuracy_target_percent": 99
    },
    
    "propulsion_model": {
      "enabled": true,
      "model_type": "Physics-based fuel cell and motor models",
      "update_frequency_hz": 1,
      "inputs": [
        "power_demand_mw",
        "altitude_ft",
        "temperature_celsius",
        "fuel_cell_age_hours"
      ],
      "outputs": [
        "fuel_consumption_kg_s",
        "efficiency_percent",
        "thrust_available_kn",
        "thermal_state"
      ],
      "predictive_capability": true
    },
    
    "trajectory_prediction": {
      "enabled": true,
      "model_type": "4D trajectory with weather integration",
      "prediction_horizon_minutes": 120,
      "update_frequency_hz": 0.016667,
      "inputs": [
        "current_state",
        "flight_plan",
        "weather_forecast",
        "atc_constraints"
      ],
      "outputs": [
        "predicted_position_4d",
        "fuel_required",
        "time_of_arrival",
        "alternate_recommendations"
      ]
    },
    
    "maintenance_prediction": {
      "enabled": true,
      "model_type": "Machine learning with physics constraints",
      "prediction_horizon_hours": 72,
      "update_frequency_hz": 0.000278,
      "monitored_systems": [
        "fuel_cells",
        "electric_motors",
        "h2_fuel_system",
        "flight_controls",
        "landing_gear",
        "avionics"
      ],
      "outputs": [
        "health_index_percent",
        "remaining_useful_life_hours",
        "failure_probability_percent",
        "recommended_actions"
      ],
      "confidence_threshold_percent": 85
    }
  },
  
  "data_recording": {
    "flight_data_recorder": {
      "integration": true,
      "parameters_recorded": 1000,
      "recording_rate_hz": 8,
      "storage_duration_hours": 25
    },
    
    "caos_data_logger": {
      "enabled": true,
      "parameters_logged": 2500,
      "high_rate_recording_hz": 100,
      "normal_rate_recording_hz": 1,
      "storage_location": "onboard_server_and_cloud",
      "retention_period_days": 365
    },
    
    "event_recording": {
      "enabled": true,
      "event_types": [
        "exceedances",
        "anomalies",
        "warnings",
        "cautions",
        "mode_changes",
        "maintenance_actions"
      ],
      "context_window_seconds": 60
    }
  },
  
  "user_interfaces": {
    "flight_deck_displays": {
      "primary_flight_display": {
        "digital_twin_overlay": true,
        "synthetic_vision": true,
        "performance_predictions": true
      },
      
      "navigation_display": {
        "trajectory_prediction": true,
        "weather_overlay": true,
        "range_rings": true,
        "alternate_airports": true
      },
      
      "systems_display": {
        "fuel_cell_status": true,
        "h2_fuel_system": true,
        "weight_and_balance": true,
        "system_health": true
      },
      
      "caos_dedicated_display": {
        "performance_optimization": true,
        "predictive_maintenance": true,
        "efficiency_monitoring": true,
        "digital_twin_status": true
      }
    },
    
    "maintenance_tablet": {
      "enabled": true,
      "functions": [
        "system_health_monitoring",
        "fault_diagnosis",
        "work_package_generation",
        "digital_twin_calibration",
        "weight_balance_updates",
        "dimension_verification"
      ]
    },
    
    "operations_control": {
      "enabled": true,
      "fleet_monitoring": true,
      "performance_tracking": true,
      "fuel_efficiency_reporting": true,
      "predictive_maintenance_scheduling": true
    }
  },
  
  "communication_interfaces": {
    "aircraft_to_ground": {
      "enabled": true,
      "methods": [
        "ACARS",
        "SATCOM",
        "VHF_datalink",
        "4G_LTE_ground"
      ],
      "data_rate_kbps": 128,
      "update_frequency_seconds": 60,
      "priority_data_immediate": true
    },
    
    "ground_to_aircraft": {
      "enabled": true,
      "updates": [
        "weather_updates",
        "atc_clearances",
        "performance_optimizations",
        "maintenance_alerts",
        "software_updates"
      ]
    },
    
    "fleet_data_sharing": {
      "enabled": true,
      "anonymized": true,
      "shared_parameters": [
        "performance_trends",
        "maintenance_events",
        "efficiency_metrics",
        "environmental_data"
      ],
      "opt_in_required": true
    }
  },
  
  "security_and_validation": {
    "data_encryption": {
      "enabled": true,
      "algorithm": "AES-256",
      "key_management": "PKI_with_certificate_authority"
    },
    
    "data_integrity": {
