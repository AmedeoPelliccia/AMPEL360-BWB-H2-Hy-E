         xsi:noNamespaceSchemaLocation="http://www.s1000d.org/S1000D_5-0/xml_schema_flat/descriptive.xsd">
  <identAndStatusSection>
    <dmAddress>
      <dmIdent>
        <dmCode modelIdentCode="AMP" systemDiffCode="A" systemCode="02"
                subSystemCode="3" subSubSystemCode="2" assyCode="00"
                disassyCode="00" disassyCodeVariant="00" infoCode="941"
                infoCodeVariant="A" itemLocationCode="A"/>
        <language languageIsoCode="en" countryIsoCode="US"/>
        <issueInfo issueNumber="004" inWork="00"/>
      </dmIdent>
      <dmAddressItems>
        <issueDate year="2025" month="11" day="05"/>
        <dmTitle>
          <techName>H2 REFUELING SYSTEM - CONSUMABLES AND MATERIALS</techName>
        </dmTitle>
      </dmAddressItems>
    </dmAddress>
    <dmStatus issueType="new">
      <security securityClassification="01"/>
      <responsiblePartnerCompany enterpriseCode="AMPEL"/>
      <originator enterpriseCode="AMPEL"/>
